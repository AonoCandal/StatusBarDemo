<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.aono.statusbardemo">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activitys.APageActivity">
        </activity>
        <activity android:name=".activitys.BPageActivity">
        </activity>
        <activity android:name=".activitys.CPageActivity">
        </activity>
        <activity android:name=".activitys.DPageActivity">
        </activity>
        <activity android:name=".activitys.EPageActivity">
        </activity>
        <activity android:name=".activitys.FPageActivity">
        </activity>
    </application>

</manifest>